       }else{\r\n            callback(new Error('请选择循环时间'));\r\n          }\r\n        }\r\n      }else{\r\n        callback();\r\n      }\r\n    }\r\n    return {\r\n      taskList: [],\r\n      addFormVisible: false, //新增界面是否显示\r\n      //新增界面数据\r\n      addForm: {\r\n        taskName: '',\r\n        type: 0,\r\n        date: '',\r\n        startTime: '',\r\n        endTime: '',\r\n        cyclic: false,\r\n        cyclicMode: 0,\r\n        cyclicDates: '',\r\n        cyclicWeeks: '',\r\n        subtaskIds: ''\r\n      },\r\n      weeks: [{\r\n        value: 1,\r\n        label: '星期一'\r\n      }, {\r\n        value: 2,\r\n        label: '星期二'\r\n      }, {\r\n        value: 3,\r\n        label: '星期三'\r\n      }, {\r\n        value: 4,\r\n        label: '星期四'\r\n      }, {\r\n        value: 5,\r\n        label: '星期五'\r\n      }, {\r\n        value: 6,\r\n        label: '星期六'\r\n      }, {\r\n        value: 7,\r\n        label: '星期日'\r\n      }],\r\n      addFormRules: {\r\n        taskName: [\r\n          { required: true, message: '请输入定时任务名称', trigger: 'blur' },\r\n          { validator: checkName, trigger: 'blur' }\r\n        ],\r\n        subtaskIds: [\r\n          { required: true, message: '请至少选择一个子任务', trigger: 'change' }\r\n        ],\r\n        date: [\r\n          { required: true, message: '请选择起止日期', trigger: 'change' }\r\n        ],\r\n        startTime: [\r\n          { required: true, message: '请选择开始时间', trigger: 'change' }\r\n        ],\r\n        endTime: [\r\n          { required: true, message: '请选择结束时间', trigger: 'change' }\r\n        ],\r\n        cyclicMode:[\r\n          { validator: checkMode, trigger: 'change' }\r\n        ]\r\n      }\r\n    }\r\n  },\r\n  computed: {\r\n    //是否跟新定时任务列表\r\n    ifUpdateTaskList:function(){\r\n      return this.$store.state.ifUpdateTimedTaskList;\r\n    },\r\n    //播放列表选项\r\n    playListOptions: function() {\r\n      return this.$store.state.subtasksList;\r\n    },\r\n    taskName: function() {\r\n      return this.addForm.taskName;\r\n    },\r\n    type: function() {\r\n      return this.addForm.type\r\n    },\r\n    mode: function() {\r\n      return this.addForm.cyclicMode\r\n    },\r\n    cyclicDate: function() {\r\n      var cyclicDate = '';\r\n      if (this.addForm.cyclicMode === 0 || this.addForm.cyclicMode === 1) {\r\n        cyclicDate = '';\r\n      } else if (this.addForm.cyclicMode === 2) {\r\n        cyclicDate = this.addForm.cyclicWeeks.join(',');\r\n      } else if (this.addForm.cyclicMode === 3) {\r\n        for (var i = 0; i < this.addForm.cyclicDates.length; i++) {\r\n          if (this.addForm.cyclicDates) {\r\n            this.addForm.cyclicDates[i] = this.addForm.cyclicDates[i].getDate()\r\n          }\r\n        }\r\n        cyclicDate = this.addForm.cyclicDates.join(',');\r\n      }\r\n      return cyclicDate;\r\n    },\r\n    strStartDate: function() {\r\n      var date = this.addForm.date[0];\r\n      return new Date(date).getTime();\r\n    },\r\n    strEndDate: function() {\r\n      var date = this.addForm.date[1];\r\n      return new Date(date).getTime();\r\n    },\r\n    strStartTime: function() {\r\n      var time = this.addForm.startTime;\r\n      return new Date(time).toTimeString().substring(0, 8);\r\n    },\r\n    strEndTime: function() {\r\n      var time = this.addForm.endTime;\r\n      return new Date(time).toTimeString().substring(0, 8);\r\n    },\r\n    subtaskIds: function() {\r\n      return this.addForm.subtaskIds.join(',');\r\n    },\r\n    params: function() {\r\n      var data = {\r\n        taskName: this.taskName,\r\n        type: this.type,\r\n        mode: this.mode,\r\n        cyclicDate: this.cyclicDate,\r\n        strStartTime: this.strStartTime,\r\n        strEndTime: this.strEndTime,\r\n        startTimestamp: this.strStartDate,\r\n        endTimestamp: this.strEndDate,\r\n        subtaskIds: this.subtaskIds\r\n      }\r\n      return data;\r\n    }\r\n  },\r\n  methods: {\r\n    //获取子任务列表\r\n    getSubtaskList() {\r