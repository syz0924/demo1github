\" :page-size=\"9\" :total=\"videoList[page]?videoList[page].length:0\" @current-change=\"changeVideoPage\"></el-pagination>\r\n            </el-row>\r\n          </contentBox>\r\n        </div>\r\n      </div>\r\n    </el-col>\r\n    <el-col :span=\"4\">\r\n      <contentBox title=\"目录\">\r\n        <div class=\"page-item\" v-for=\"(item,index) in pageList\">\r\n          <div class=\"button fillcontain\" @click=\"changePage(index)\">{{item}}</div>\r\n        </div>\r\n      </contentBox>\r\n    </el-col>\r\n    <el-dialog :visible.sync=\"addFormVisible\" center width=\"500px\">\r\n      <contentBox title=\"添加子任务\">\r\n        <div class=\"dialog-content\">\r\n          <el-form :model=\"addForm\">\r\n            <el-form-item class=\"flex\" label=\"任务名称\">\r\n              <el-input v-model=\"addForm.subtaskName\" placeholder=\"请输入任务名称\" clearable></el-input>\r\n            </el-form-item>\r\n          </el-form>\r\n          <div slot=\"footer\" class=\"dialog-footer\">\r\n            <div class=\"button\" @click=\"addFormVisible = false\">取消</div>\r\n            <div class=\"button submit\" @click=\"addSubmit\">提交</div>\r\n          </div>\r\n        </div>\r\n      </contentBox>\r\n    </el-dialog>\r\n  </el-row>\r\n</template>\r\n<script>\r\nimport { getFileList, addSubtask, sendVideoSplit, sendEdlmxSplit, uploadFile } from '@/api/api.js';\r\nimport playControl from '@/components/playControl';\r\nimport playerBackground from '@/assets/player-bg.png';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      playerBG: {\r\n        backgroundImage: 'url(' + playerBackground + ')'\r\n      },\r\n      videoParams: { //上传视频的参数\r\n        minute: '' //视频播放时长\r\n      },\r\n      fileList: [],\r\n      lightList: [],\r\n      videoList: [],\r\n      pageList: ['目录一', '目录二', '目录三', '目录四', '目录五', '目录六', '目录七', '目录八', '目录九', '目录十', '目录十一', '目录十二', '目录十三', '目录十四', '目录十五'],\r\n      page: 0,\r\n      filePage: 1,\r\n      lightPage: 1,\r\n      videoPage: 1,\r\n      pageSize: 0,\r\n      addFormVisible: false,\r\n      addForm: { //添加子任务参数\r\n        subtaskName: '', //子任务名称\r\n        filePath: '', //文件路径\r\n        time: '', //文件播放时长\r\n        md5: '', //文件md5值\r\n        typeId: '' //文件类型:0--灯光文件；1--视频文件\r\n      }\r\n    }\r\n  },\r\n  computed: {\r\n    fileShowList: function() {\r\n      return this.fileList.slice((this.filePage - 1) * this.pageSize, this.filePage * this.pageSize);\r\n    },\r\n    lightShowList: function() {\r\n      var lightList = this.lightList[this.page];\r\n      if (lightList) {\r\n        var lightShowList = lightList.slice((this.lightPage - 1) * 9, this.lightPage * 9);\r\n        return lightShowList;\r\n      } else {\r\n        return [];\r\n      }\r\n    },\r\n    videoShowList: function() {\r\n      var videoList = this.videoList[this.page];\r\n      if (videoList) {\r\n        var videoShowList = videoList.slice((this.videoPage - 1) * 9, this.videoPage * 9);\r\n        return videoShowList;\r\n      } else {\r\n        return [];\r\n      }\r\n    }\r\n  },\r\n  components: {\r\n    playControl\r\n  },\r\n  methods: {\r\n    //获取文件列表\r\n    getFileList() {\r\n      getFileList().then(res => {\r\n        console.log(res)\r\n        if (res.result === 'send successful') {\r\n          var list = JSON.parse(res.directoryList);\r\n          console.log(list)\r\n          var arr = [];\r\n          var lightList = [];\r\n          var videoList = [];\r\n          for (var i = 0; i < list.length; i++) {\r\n            arr = arr.concat(list[i].lightFileList);\r\n            arr = arr.concat(list[i].videoFileList);\r\n            lightList.push(list[i].lightFileList);\r\n            videoList.push(list[i].videoFileList);\r\n          }\r\n          this.fileList = arr;\r\n          this.lightList = lightList;\r\n          this.videoList = videoList;\r\n        }\r\n      });\r\n    },\r\n\r\n    // 改变文件列表页码\r\n    changeFilePage(page) {\r\n      this.filePage = page;\r\n    },\r\n\r\n    // 改变灯光