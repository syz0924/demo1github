n        return;\r\n      }\r\n      this.instantTaskVisible = type === 'instant' ? true : false;\r\n      this.timerTaskVisible = type === 'timer' ? true : false;\r\n\r\n      this.nodeList = list;\r\n    },\r\n\r\n    /**\r\n     * 将选择的节点数据组装成发给后端的参数\r\n     * @param  {String} type 要发送的任务类型：instant--即时任务；timer--定时任务\r\n     * @param  {Array} arr1  选择的网关列表\r\n     * @param  {Array} arr2  选择的485列表\r\n     * @param  {Array} arr3  选择的继电器列表\r\n     * @param  {Array} arr4  选择的回路列表\r\n     * @return {Object}      发给后端的参数\r\n     */\r\n    getTaskParams(type, arr1, arr2, arr3, arr4) {\r\n      console.log(arr2)\r\n      var params;\r\n      var rackId = arr1.join(',');\r\n      var taskMsg = {}\r\n      taskMsg.delaymsc = type === 'instant' ? this.instantTaskForm.delay + '' : this.timerTaskForm.delay + '';\r\n      var taskList = [];\r\n      for (var i = 0; i < arr2.length; i++) {\r\n        var data1 = arr2[i].split(',');\r\n        taskList[i] = {};\r\n        taskList[i].rs485toNetIP = data1[0];\r\n        taskList[i].port = data1[1];\r\n        taskList[i].rackId = parseInt(data1[3]);\r\n        taskList[i].gatewayMac = data1[2];\r\n        taskList[i].tasks = [];\r\n        for (var j = 0; j < arr3.length; j++) {\r\n          var data2 = arr3[j].split(',');\r\n          var loopNums = [];\r\n          for (var k = 0; k < arr4.length; k++) {\r\n            if (arr4[k].rs485toNetIP === data2[0] && arr4[k].slaveAddr == data2[1]) {\r\n              loopNums.push(arr4[k].loopNums)\r\n            }\r\n          }\r\n          if (data1[0] === data2[0]) {\r\n            taskList[i].tasks.push({\r\n              slaveAddr: data2[1],\r\n              loopNums: loopNums.join(','),\r\n              state: type === 'instant' ? this.instantTaskForm.state : '1',\r\n              relayId: data2[2]\r\n            })\r\n          }\r\n        }\r\n      }\r\n      if (type === 'instant') {\r\n        var saveState = this.instantTaskForm.saveTask;\r\n        var taskName = this.instantTaskForm.saveTask ? this.instantTaskForm.taskName : '';\r\n        taskMsg.realTimeTasks = taskList;\r\n        params = { rackId, saveState, taskName, taskMsg }\r\n      } else if (type === 'timer') {\r\n        taskMsg.dayofweek = this.timerTaskForm.dayofweek.join(',');\r\n        taskMsg.startTime = this.timerTaskForm.startTime.toTimeString().substring(0, 8);\r\n        taskMsg.endTime = this.timerTaskForm.endTime.toTimeString().substring(0, 8);\r\n        taskMsg.timlyTasks = taskList;\r\n        var taskName = this.timerTaskForm.taskName;\r\n        params = { rackId, taskMsg, taskName };\r\n      }\r\n      return params;\r\n    },\r\n\r\n    //获取选择的节点,并将列表拆成网关、485、继电器、回路4个列表数组\r\n    getSelectedNode() {\r\n      var rackIdArr = []\r\n      var rs485toNetIPArr = []\r\n      var slaveAddrArr = []\r\n      var loopNumsArr = [];\r\n      var list = this.nodeList;\r\n      for (var i = 0; i < list.length; i++) {\r\n        if (list[i]) {\r\n          rackIdArr.push(JSON.parse(list[i]).rackId)\r\n          rs485toNetIPArr.push(JSON.parse(list[i]).rs485toNetIP + ',' + JSON.parse(list[i]).port + ',' + JSON.parse(list[i]).mac + ',' + JSON.parse(list[i]).rackId)\r\n          slaveAddrArr.push(JSON.parse(list[i]).rs485toNetIP + ',' + JSON.parse(list[i]).slaveAddr + ',' + JSON.parse(list[i]).relayId)\r\n          loopNumsArr.push(JSON.parse(list[i]))\r\n        }\r\n      }\r\n      // 去掉数组中重复的项\r\n      rackIdArr = unique1(rackIdArr)\r\n      rs485toNetIPArr = unique1(rs485toNetIPArr)\r\n      slaveAddrArr = unique1(slaveAddrArr)\r\n      var arr = [rackIdArr, rs485toNetIPArr, slaveAddrArr, loopNumsArr]\r\n      return arr;\r\n    },\r\n\r\n    //提交即时任务\r\n    submitInstantTask(formName) {\r\n      //验证表单填写是否规范\r\n      this.$refs[formName].validate((valid) => {\r\n        if (valid) {\r\n          const that = this;\r\n          var arr = this.getSelectedNode();\r\n          var params = this.getTaskParams('instant', arr[0], arr[1], arr[2], arr[3]);\r\n          // 如