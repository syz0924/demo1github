public enum EnumErrType {

	OffLine(0, "离线"), Task(1, "任务"), Lmx(2, "灯光文件"), Video(2, "视频文件");

	private Integer key;

	private String value;

	EnumErrType(Integer key, String value) {
		this.key = key;
		this.value = value;
	}

	public Integer getKey() {
		return key;
	}

	public void setKey(Integer key) {
		this.key = key;
	}

	public String getValue() {
		return value;
	}

	public void setValue(String value) {
		this.value = value;
	}
}
