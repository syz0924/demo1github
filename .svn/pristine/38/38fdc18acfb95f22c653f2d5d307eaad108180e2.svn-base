public enum EnumDeviceType {

	RTR(0, "RTR服务器"), DMX512(1, "控制盒"), Sprite(2, "录放精灵");

	private Integer key;

	private String value;

	EnumDeviceType(Integer key, String value) {
		this.key = key;
		this.value = value;
	}

	public Integer getKey() {
		return key;
	}

	public void setKey(Integer key) {
		this.key = key;
	}

	public String getValue() {
		return value;
	}

	public void setValue(String value) {
		this.value = value;
	}
}
