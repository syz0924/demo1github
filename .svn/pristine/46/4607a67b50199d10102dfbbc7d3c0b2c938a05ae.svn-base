.lovdmx-shade {
	position: fixed;
	width: 100%;
	height: 100%;
	top: 0;
	left: 0;
}

/*lovdmx-popup-window start*/
.lovdmx-popup-window {
	position: fixed;
	width: 500px;
	height: 450px;
	top: 264px;
	left: 377px;
	margin: 0;
	padding: 0;
	background-color: #fff;
	border-radius: 2px;
	box-shadow: 1px 1px 50px rgba(0, 0, 0, .3);
	pointer-events: auto;
	-webkit-overflow-scrolling: touch;
	-webkit-background-clip: content;
	z-index: 99999;
}
#lovdmx-popup-window .table{
	width: 72%;
}
.lovdmx-title {
	padding: 0 80px 0 20px;
	height: 42px;
	line-height: 42px;
	border-bottom: 1px solid #eee;
	font-size: 14px;
	color: #333;
	overflow: hidden;
	background-color: #F8F8F8;
	border-radius: 2px 2px 0 0;
}

.lovdmx-admin .lovdmx-title {
	height: 50px;
	line-height: 50px;
	border: 0;
	background-color: #03152A !important;
	color: white;
}

.lovdmx-admin .lovdmx-content {
	position: relative;
	padding: 20px;
	overflow: auto;
}

.lovdmx-form-item {
	margin-bottom: 15px;
	clear: both;
	zoom: 1;
}

.lovdmx-form button.lovdmx-select-btn{
	width: 36%;
	height:30px;
	line-height:30px;
	margin-top:2px;
	background-color: #dcdbdbb5;
	cursor: pointer;
}

.lovdmx-form button.active{
	background-color: rgba(225,155,0);
}

.lovdmx-form .lovdmx-select-btn:not(:last-child){
	border-right:1px solid #fff;
}

.lovdmx-select-btn:first-child{
	border-radius:8px 0 0 8px;
}
.lovdmx-select-btn:last-child{
	border-radius:0 8px 8px 0;
}




.lovdmx-input:focus{
	border-color:#C9C9C9!important
}
.lovdmx-form-danger:focus{
	border-color:#FF5722!important;
}

.lovdmx-form-item .lovdmx-form-lable {
	display: inline-block;
	float:left;
	padding: 9px 15px;
	width: 120px;
	font-weight: 400;
	line-height: 20px;
	text-align: right;
}

.lovdmx-form-item .lovdmx-input-inline {
	display: inline-block;
	width: 190px;
	margin-right: 10px;
	vertical-align: middle;
}

.lovdmx-word-aux{
	position: relative;
	color: #999!important;
	color: 0 5!important;
}

.lovdmx-form-mid{
	display: inline-block;
	padding: 9px 0!important;
	line-height: 20px;
	margin-right: 10px;
}


.lovdmx-form input[type=radio], .lovdmx-form .select, .lovdmx-form input[type=checkbox]
	{
	display: none;
}

.lovdmx-form-radio {
	display: inline-block;
	vertical-align: middle;
	line-height: 28px;
	margin: 6px 10px 0 0;
	padding-right: 10px;
	cursor: pointer;
	font-size: 0;
}

.lovdmx-form-radio .lovdmx-icon {
	display: inline-block;
}


.lovdmx-selectbox{height: 220px;}

.lovdmx-selectbox .select-bar{
	padding: 0 20px;
}
.select-bar span{
	height: 100%;
	margin-left: 10px;
	margin-top: 12px;
	text-align: center;
}
.select-bar select{
	display: block;
	width: 150px;
	height: 180px;
	border:1px solid #dedcdc;
	padding: 4px;
	font-size: 14px;
}

.lovdmx-selectbox .btn-bar{
	padding: 18px 0; 
}

.lovdmx-selectbox .btn-bar p{
	padding-bottom: 10px;
}
.lovdmx-selectbox .btn-bar .lovdmx-btn{
	width: 50px;
	height: 38px;
	line-height: 38px;
	font-size: 14px;
	cursor: pointer;
	border-radius: 5px;
	text-align: center;
}

.lovdmx-input-block{
	position: relative;
	margin-left: 110px;
	min-height: 36px;
}

.lovdmx-form-checkbox{
	position: relative;
    height: 30px;
    line-height: 30px;
    margin-right: 10px;
    padding-right: 30px;
    cursor: pointer;
    font-size: 0;
    -webkit-transition: .1s linear;
    transition: .1s linear;
    box-sizing: border-box;
}

.lovdmx-form-item .primary{
	min-width: 18px;
	min-height: 18px;
	height: auto!important;
	line-height: normal!important;
	margin-top: 10px;
	margin-right: 0;
	padding-left: 28px;
	padding-right: 0;
	border:none!important;
	background:0 0;
}

.lovdmx-upload-img {
    width: 92px;
    height:92px;
    margin: 0 10px 10px 0;
    cursor: pointer;
}
.lovdmx-form-checkbox span{
	padding: 0 10px;
    height: 100%;
    font-size: 14px;
    border-radius: 2px 0 0 2px;
    background-color: #d2d2d2;
    color: #fff;
    overflow: hidden;
}
.lovdmx-form-item .primary span{
	padding-left: 0;
    padding-right: 15px;
    line-height: 18px;
    background: 0 0;
    color: #666;
}

.lovdmx-form-switch{
	position: relative;
	display:inline-block;
    height: 22px;
    line-height: 22px;
    min-width: 35px;
    padding: 0 5px;
    margin-top: 8px;
    border: 1px solid #d2d2d2;
    border-radius: 20px;
    cursor: pointer;
    background-color: #fff;
    -webkit-transition: .1s linear;
    transition: .1s linear;
}
.lovdmx-form-onswitch{
	border-color:#5FB878;
	background-color:#5FB878;
}
.lovdmx-form-switch em{
	position: relative;
    top: 0;
    width: 25px;
    font-size: 12px;
    margin-left: 21px;
    padding: 0!important;
    text-align: center!important;
    color: #999!important;
    font-style: normal!important;
}
.lovdmx-form-onswitch em {
    margin-left: 5px;
    margin-right: 21px;
    color: #fff!important;
}

.lovdmx-form-switch i{
	position: absolute;
    left: 5px;
    top: 2px;
    width: 16px;
    height: 16px;
    border-radius: 20px;
    background-color: #d2d2d2;
    -webkit-transition: .1s linear;
    transition: .1s linear;
}

.lovdmx-form-onswitch i {
    left: 100%;
    margin-left: -21px;
    background-color: #fff;
}

.lovdmx-form-checkbox i{
	position: absolute;
    right: 0;
    top: 0;
    width: 30px;
    height: 28px;
    border: 1px solid #d2d2d2;
    border-left: none;
    border-radius: 0 2px 2px 0;
    color: #fff;
    font-size: 20px;
    text-align: center;
}
.primary i{
	right: auto;
    left: 0;
    width: 16px;
    height: 16px;
    line-height: 16px;
    border: 1px solid #d2d2d2;
    font-size: 12px;
    border-radius: 2px;
    background-color: #fff;
    -webkit-transition: .1s linear;
    transition: .1s linear;
}

.lovdmx-textarea{
	position: relative;
	display: block;
	width: 96%;
    min-height: 100px;
    height: auto;
    line-height: 20px;
    padding: 6px 10px;
    resize: vertical;
    border-width: 1px;
    border-style: solid;
    background-color: #fff;
    border-radius: 2px;
}


.table-parameter tr td,.table-parameter tr th{
	border:1px solid #e6b176;
}

.lovdmx-form-radio>i {
	margin-right: 8px;
	font-size: 22px;
	color: #c2c2c2;
}

.lovdmx-form-radio>i:hover, .lovdmx-form-radioed>i {
	color: #5FB878;
}

.lovdmx-form-radio * {
	font-size: 14px;
}

.lovdmx-form-radio, .lovdmx-form-radio * {
	display: inline-block;
	vertical-align: middle;
}

.lovdmx-upload-file {
	display: none !important;
	opacity: .01;
}

.lovdmx-popup-window .close {
	position: absolute;
	padding: 5px;
	right: 10px;
	top: 8px;
	font-size: 26px;
	color: #fff;
	cursor: pointer;
}


.lovdmx-upload-preview ul.add-picture li{
	position: relative;
}

.lovdmx-upload-preview ul.add-picture .remove-img{
	position: absolute;
	top: -16px;
	right:0;
	color:red;
	cursor: pointer;
}

.lovdmx-setwin {
	position: absolute;
	right: 15px;
	top: 15px;
	font-size: 0;
	line-height: initial;
}

.lovdmx-resize {
	position: absolute;
	width: 15px;
	height: 15px;
	bottom: 0;
	right: 0;
	cursor: se-resize;
}
/*lovdmx-popup-window end*/
/*lovdmx-table-tips start*/
.lovdmx-layer{
    -webkit-overflow-scrolling: touch;
    top: 150px;
    left: 0;
    margin: 0;
    padding: 0;
    background-color: #fff;
    -webkit-background-clip: content;
    border-radius: 2px;
    box-shadow: 1px 1px 50px rgba(0,0,0,.3);
}
.lovdmx-layer-tips {
    background: 0 0;
    box-shadow: none;
    border: none;
}
.lovdmx-layer-content {
    position: relative;
}
.lovdmx-layer-tips .lovdmx-layer-content {
    position: relative;
    line-height: 22px;
    min-width: 12px;
    padding: 8px 15px;
    font-size: 12px;
    _float: left;
    border-radius: 2px;
    box-shadow: 1px 1px 3px rgba(0,0,0,.2);
    background-color: #000;
    color: #fff;
}
body .lovdmx-table-tips .lovdmx-layer-content {
    background: 0 0;
    padding: 0;
    box-shadow: 0 1px 6px rgba(0,0,0,.1);
}
.lovdmx-table-tips-main{
	border-width: 1px;
    border-style: solid;
    border-color: #e6e6e6;
}
.lovdmx-table-tips-main {
    margin: -44px 0 0 -1px;
    max-height: 150px;
    max-width: 350px;
    padding: 8px 15px;
    font-size: 12px;
    overflow-y: scroll;
    background-color: #fff;
    color: #333;
}
.lovdmx-table-tips-c {
    position: absolute;
    right: -3px;
    top: -12px;
    width: 18px;
    height: 18px;
    line-height:18px;
    text-align: center;
    font-weight: 700;
    border-radius: 100%;
    font-size: 18px;
    cursor: pointer;
    background-color: #666;
}
.lovdmx-layer-setwin {
    position: absolute;
    right: 15px;
    *right: 0;
    top: 15px;
    font-size: 0;
    line-height: initial;
}
/*lovdmx-table-tips end*/


