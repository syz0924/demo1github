//添加背景
function addBackground(){
	var background="";
	background +="<div class='lovdmx-shade' id='lovdmx-shade2' style='z-index: 9999; background-color: rgb(0, 0, 0); opacity: 0.3;'></div>";
	$("body").append(background);
}


//添加图片类型
function addParameterImgType(productContent,parameter,path) {
	var addImg="";
	addImg +="<div class='lovdmx-form-item none new-parameter upload-parameter"+parameter.id+"' data-id='"+productContent.id+"'>";
		addImg +="<label class='lovdmx-form-lable'>"+parameter.name+"</label>";
		addImg +="<div class='lovdmx-upload-preview'>";
			addImg +="<ul class='fl_child clearFix'>";
				addImg +="<li><input type='file' name='file' hidden='hidden' onchange='uploadPicture(this)'><img src='"+path+"/"+productContent.img+"' class='lovdmx-upload-img upload'>";
				addImg +="</li>";
			addImg +="</ul>";
		addImg +="</div>";
	addImg +="</div>";
	
	return addImg;
}
// 添加性能描述类型
function addParameterPerformanceType(productContent,parameter) {
	var addPerformance="";
	
	addPerformance +="<div class='lovdmx-form-item none new-parameter upload-parameter"+parameter.id+"'  data-id='"+productContent.id+"'>";
		addPerformance +="<label class='lovdmx-form-lable'>"+parameter.name+"</label>";
		addPerformance +="<table class='table table-parameter'>";
			addPerformance +="<thead>";
				addPerformance +="<tr>";
					addPerformance +="<th>";
						addPerformance +="<button class='lovdmx-btn' id='add-performance' type='button'>添加特点</button>";
					addPerformance +="</th>";
					addPerformance +="<th>操作</th>";
				addPerformance +="</tr>";
			addPerformance +="</thead>";
			addPerformance +="<tbody>";
			
			for(var i=0;i<productContent.contentList.length;i++){
				var value = productContent.contentList[i];
				addPerformance +="<tr>";
					addPerformance +="<td><input type='text' name='content' value='"+value+"' placeholder='请输入 注释' class='lovdmx-input'></td>";
					addPerformance +="<td><a class='lovdmx-btn lovdmx-btn-del lovdmx-btn-xs  remove-table-tr'><i class='icon-shanchu lovdmx-icon iconfont '></i>删除</a></td>";
				addPerformance +="</tr>";
			}
				
			addPerformance +="</tbody>";
			
			addPerformance +="<tfoot>";
				addPerformance +="<tr>";
					addPerformance +="<td colspan='2'><input type='text' name='annotation' value='"+productContent.annotation+"'  placeholder='请输入规格参数' class='lovdmx-input'></td>";
				addPerformance +="</tr>";
			addPerformance +="</tfoot>";
		addPerformance +="</table>";
	addPerformance +="</div>";
	
	return addPerformance;
}

// 添加规格类型
function addParameterSpecificationType(productContent,parameter) {
	var specificationList=productContent.specificationList;
	var addSpecification="";
	
	addSpecification +="<div class='lovdmx-form-item none new-parameter upload-parameter"+parameter.id+"' data-id='"+productContent.id+"'>";
		addSpecification +="<label class='lovdmx-form-lable'>"+parameter.name+"</label>";
		addSpecification +="<table class='table table-parameter'>";
			addSpecification +="<thead>";
				addSpecification +="<tr>";
					addSpecification +="<th colspan='2'>";
					addSpecification +="<button class='lovdmx-btn' id='add-specification' type='button'>添加规格</button>";
					addSpecification +="</th>";
					addSpecification +="<th>操作</th>";
				addSpecification +="</tr>";
			addSpecification +="</thead>";
			addSpecification +="<tbody>";
				for(var i=0;i<specificationList.length;i++){
					var value =specificationList[i];
					var split = value.split("&!&");
					addSpecification +="<tr>";
						for(var j=0;j<split.length;j++){
							addSpecification +="<td><input type='text' name='username' value='"+split[j]+"' placeholder='请输入规格参数' class='lovdmx-input'></td>";
						}
						addSpecification +="<td><a class='lovdmx-btn lovdmx-btn-del lovdmx-btn-xs  remove-table-tr'><i class='icon-shanchu lovdmx-icon iconfont '></i>删除</a></td>";
					addSpecification +="</tr>";
				}
			addSpecification +="</tbody>";
		addSpecification +="</table>";
	addSpecification +="</div>";
	
	return addSpecification;
}

// 添加文件类型
function addParameterFileType(productContent,parameter,path) {
	var addFile = "";
	
	addFile +="<div class='lovdmx-form-item none new-parameter upload-parameter"+parameter.id+"'  data-id='"+productContent.id+"'>";
		addFile +="<label class='lovdmx-form-lable'>"+parameter.name+"</label>";
		addFile +="<div class='lovdmx-input-inline'><input type='text' placeholder='请上传文件' value='"+path+"/"+productContent.file+"' class='lovdmx-input'></div>";
		addFile +="<input type='file' name='file' onchange='uploadFile(this)'  class='lovdmx-upload-file'>";
		addFile +="<button class='lovdmx-btn upload-file' type='button'>上传图片</button>";
	addFile +="</div>";
	
	return addFile;
}



