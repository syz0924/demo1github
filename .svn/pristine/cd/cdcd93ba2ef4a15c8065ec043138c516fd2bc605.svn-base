public class Role implements Serializable {
	// @Fields serialVersionUID : TODO
	private static final long serialVersionUID = -4746072304961181256L;
	// 编号
	private Integer roleId;
	// 角色名
	private String roleName;
	// 角色权限
	private String roleLimit;
	// 角色权限名称
	private String limitNames;

	public Integer getRoleId() {
		return roleId;
	}

	public void setRoleId(Integer roleId) {
		this.roleId = roleId;
	}

	public String getRoleName() {
		return roleName;
	}

	public void setRoleName(String roleName) {
		this.roleName = roleName == null ? null : roleName.trim();
	}

	public String getRoleLimit() {
		return roleLimit;
	}

	public void setRoleLimit(String roleLimit) {
		this.roleLimit = roleLimit;
	}

	public String getLimitNames() {
		return limitNames;
	}

	public void setLimitNames(String limitNames) {
		this.limitNames = limitNames;
	}
}